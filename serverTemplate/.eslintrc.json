{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
        // "plugin:@typescript-eslint/recommended",
    ],
    // "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        // "@typescript-eslint"
    ],
    "rules": {
        "no-console": "off",
        "prettier/prettier": [
            "error",
            {
                "singleQuote": true
            }
        ]
    }
}
